Package: LUMCONplants
Type: Package
Title: Allometry and Plant Dynamics in Louisiana Universities Marine Consortium Salt Marshes
Version: 0.0.1
Authors@R: c(
    person(c("Troy", "D."), "Hill", role = c("aut", "cre"), 
    email = "Hill.Troy@gmail.com", comment = c(ORCID = "0000-0003-2980-4099")),
